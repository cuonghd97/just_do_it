<%= provide(:title, "Đăng ký") %>
<%= form_for @user, url: signup_path do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :fullname %>
  <%= f.text_field :fullname %>

  <%= f.submit "Đăng ký" %>
<% end %>